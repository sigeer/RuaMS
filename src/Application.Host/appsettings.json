{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  // 只能是ConnectionStrings里的键
  "Database": "Sqlite",
  "ConnectionStrings": {
    "MySql": "server=localhost;user id=root;password=root;SslMode=None;allowPublicKeyRetrieval=true;database=ruams;Charset=utf8mb4;",
    "Sqlite": "Data Source=database.db"
  },
  // 生成long型id种子，不可重复
  "LongIdSeed": 1,
  // 服务器名
  "ServerName": "MasterServer",
  // 开放Api 用于后台管理
  "EnableOpenApi": false,
  // 对外开放Api端口
  "OpenApiPort": 8080,
  "UseExtraChannel": false,
  // Grpc服务端口 默认无效 UseExtraChannel=true时生效
  "GrpcPort": 7878,
  // 频道设置，IsStandalone编译有效
  "ChannelServerConfig": {
    // 同进程部署时，会使用LocalChannelServerTransport，不会走Grpc，也就用不上这个配置
    // "MasterServerGrpcAddress": "http://127.0.0.1:7878",
    // 不可重复，用于区分频道服务器
    "ServerName": "ChannelLocal",
    // Channel的Host会返回给客户端，供客户端连接
    "ServerHost": "127.0.0.1",
    // 频道设置
    "ChannelConfig": [
      {
        "Port": 7575
      },
      {
        "Port": 7576
      },
      {
        "Port": 7577
      }
    ],
    "SystemConfig": {
      "AutoClearMap": true
    }
  },
  "GameConfig": {
    "Name": "RuaMS",
    "ServerMessage": "Welcome to Scania!",
    "EventMessage": "Scania!",
    "RecommendMessage": "Welcome to Scania!",
    "MobRate": 1,
    "ExpRate": 10,
    "DropRate": 10,
    "BossDropRate": 10,
    "MesoRate": 10,
    "QuestRate": 5,
    "TravelRate": 10,
    "FishingRate": 10
  }
}
